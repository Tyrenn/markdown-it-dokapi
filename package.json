{
	"name": "@tyren/markdown-it-dokapi",
	"version": "0.0.9",
	"description": "A markdown-it plugin providing tools to document API",
	"main": "dist/src/index.js",
	"types": "dist/src/index.d.ts",

	"contributes": {
		"markdown.markdownItPlugins": true,
		"markdown.previewStyles": [
			"styles/index.css"
		]
	},
	"scripts": {
