		"markdown-it": "^13.0.1",
		"typescript": "^3.1.4"
	},
	"bugs": {
		"url": "https://github.com/tyrenn/markdown-it-dokapi/issues"
	},
	"homepage": "https://github.com/tyrenn/markdown-it-dokapi#readme"
